.alert {
  position: fixed;
  z-index: 3;
  bottom: 50px;
  left: 100%;
  transform: translateX(0%);
  width: 50%;
  padding: 5px;
  padding-left: 20px;
  background-color: #F6F7F8;
  color: #131316;
  font-weight: bold;
  text-align: left;
  border-radius: 10px;
  border-inline: solid 7px #131316;
  box-shadow: 1px 1px 10px black;
  animation: alertShow 2s ease-out 1 forwards;
}
.alert-success {
  color: #138A36;
  border-inline: solid 7px #138A36;
}
.alert-error {
  color: #C6352F;
  border-inline: solid 7px #C6352F;
}
.alert-alert {
  color: #CBA328;
  border-inline: solid 7px #CBA328;
}
.alert-close {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20px;
  width: 25px;
  height: 25px;
  background-image: url("./img/letra-x.png");
  background-color: transparent;
  background-size: contain;
  background-repeat: no-repeat;
  border: none;
}

@keyframes alertShow {
  0% {
    opacity: 0;
    left: 100%;
    transform: translateX(0%);
  }
  50% {
    opacity: 1;
    left: 45%;
    transform: translateX(-50%);
  }
  70% {
    left: 55%;
    transform: translateX(-50%);
  }
  90% {
    left: 49%;
    transform: translateX(-50%);
  }
  100% {
    left: 50%;
    transform: translateX(-50%);
  }
}
